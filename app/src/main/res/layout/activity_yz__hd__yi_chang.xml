<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#888"
    android:orientation="vertical"
    tools:context="com.ge.med.mobile.nursing.ui.activity.YZ_HD_YiChangActivity">
    <include layout="@layout/biao_ti_lan_item"/>
    <include layout="@layout/yi_zhu_tou_tiao_mu_include"/>
    <RelativeLayout
        android:id="@+id/yi_zhu_zhi_xing_frame_layout"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <Button
            android:id="@+id/yz_zx_zhu_she_xiang_qing_bt"
            android:text="查看药品详情"
            android:textSize="20sp"
            android:textColor="#fff"
            android:layout_marginTop="10dp"
            android:layout_marginRight="5dp"
            android:layout_marginLeft="5dp"
            android:background="@drawable/login_bt_slt"
            android:layout_width="match_parent"
            android:layout_height="45dp" />
        <TextView
            android:layout_below="@+id/yz_zx_zhu_she_xiang_qing_bt"
            android:text="医嘱执行前检查"
            android:textSize="14sp"
            android:textColor="#99ffffff"
            android:gravity="center"
            android:layout_marginTop="5dp"
            android:layout_marginRight="5dp"
            android:layout_marginLeft="5dp"
            android:layout_width="match_parent"
            android:layout_height="20dp"/>
        <LinearLayout
            android:id="@+id/yz_zx_zhu_she_ll1"
            android:layout_below="@+id/yz_zx_zhu_she_xiang_qing_bt"
            android:orientation="horizontal"
            android:layout_marginTop="25dp"
            android:layout_width="match_parent"
            android:layout_height="45dp">
            <CheckBox
                android:id="@+id/yz_zx_zhu_she_cb1_1"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:button="@null"
                android:text="药品过期"
                android:gravity="center"
                android:textSize="20sp"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="1dp"
                android:background="@drawable/yichang_bg_shape1"
                android:layout_height="match_parent"
                android:textColor="@color/abc_secondary_text_material_light" />
            <CheckBox
                android:id="@+id/yz_zx_zhu_she_cb1_2"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:button="@null"
                android:text="配伍禁忌"
                android:gravity="center"
                android:textSize="20sp"
                android:background="@drawable/yichang_bg_shape2"
                android:layout_height="match_parent"
                android:textColor="@color/abc_secondary_text_material_light" />
            <CheckBox
                android:id="@+id/yz_zx_zhu_she_cb1_3"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:button="@null"
                android:text="药品变质"
                android:gravity="center"
                android:textSize="20sp"
                android:layout_marginLeft="1dp"
                android:layout_marginRight="5dp"
                android:background="@drawable/yichang_bg_shape3"
                android:layout_height="match_parent"
                android:textColor="@color/abc_secondary_text_material_light" />
        </LinearLayout>
        <LinearLayout
            android:id="@+id/yz_zx_zhu_she_ll2"
            android:layout_below="@+id/yz_zx_zhu_she_ll1"
            android:orientation="horizontal"
            android:layout_marginTop="1dp"
            android:layout_width="match_parent"
            android:layout_height="45dp">
            <CheckBox
                android:id="@+id/yz_zx_zhu_she_cb2_1"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:button="@null"
                android:text="安瓿破碎"
                android:gravity="center"
                android:textSize="20sp"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="1dp"
                android:background="@drawable/yichang_bg_shape2"
                android:layout_height="match_parent"
                android:textColor="@color/abc_secondary_text_material_light" />
            <CheckBox
                android:id="@+id/yz_zx_zhu_she_cb2_2"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:button="@null"
                android:text="瓶盖松动"
                android:gravity="center"
                android:textSize="20sp"
                android:background="@drawable/yichang_bg_shape2"
                android:layout_height="match_parent"
                android:textColor="@color/abc_secondary_text_material_light" />
            <CheckBox
                android:id="@+id/yz_zx_zhu_she_cb2_3"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:button="@null"
                android:text="床号错误"
                android:gravity="center"
                android:textSize="20sp"
                android:layout_marginLeft="1dp"
                android:layout_marginRight="5dp"
                android:background="@drawable/yichang_bg_shape2"
                android:layout_height="match_parent"
                android:textColor="@color/abc_secondary_text_material_light" />
        </LinearLayout>
        <LinearLayout
            android:id="@+id/yz_zx_zhu_she_ll3"
            android:layout_below="@+id/yz_zx_zhu_she_ll2"
            android:orientation="horizontal"
            android:layout_marginTop="1dp"
            android:layout_width="match_parent"
            android:layout_height="45dp">
            <CheckBox
                android:id="@+id/yz_zx_zhu_she_cb3_1"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:button="@null"
                android:text="姓名错误"
                android:gravity="center"
                android:textSize="20sp"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="1dp"
                android:background="@drawable/yichang_bg_shape2"
                android:layout_height="match_parent"
                android:textColor="@color/abc_secondary_text_material_light" />
            <CheckBox
                android:id="@+id/yz_zx_zhu_she_cb3_2"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:button="@null"
                android:text="药品错误"
                android:gravity="center"
                android:textSize="20sp"
                android:background="@drawable/yichang_bg_shape2"
                android:layout_height="match_parent"
                android:textColor="@color/abc_secondary_text_material_light" />
            <CheckBox
                android:id="@+id/yz_zx_zhu_she_cb3_3"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:button="@null"
                android:text="剂量错误"
                android:gravity="center"
                android:textSize="20sp"
                android:layout_marginLeft="1dp"
                android:layout_marginRight="5dp"
                android:background="@drawable/yichang_bg_shape2"
                android:layout_height="match_parent"
                android:textColor="@color/abc_secondary_text_material_light" />
        </LinearLayout>
        <LinearLayout
            android:id="@+id/yz_zx_zhu_she_ll4"
            android:layout_below="@+id/yz_zx_zhu_she_ll3"
            android:orientation="horizontal"
            android:layout_marginTop="1dp"
            android:layout_width="match_parent"
            android:layout_height="45dp">
            <CheckBox
                android:id="@+id/yz_zx_zhu_she_cb4_1"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:button="@null"
                android:text="时间错误"
                android:gravity="center"
                android:textSize="20sp"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="1dp"
                android:background="@drawable/yichang_bg_shape4"
                android:layout_height="match_parent"
                android:textColor="@color/abc_secondary_text_material_light" />
            <CheckBox
                android:id="@+id/yz_zx_zhu_she_cb4_2"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:button="@null"
                android:text="浓度错误"
                android:gravity="center"
                android:textSize="20sp"
                android:background="@drawable/yichang_bg_shape2"
                android:layout_height="match_parent"
                android:textColor="@color/abc_secondary_text_material_light" />
            <CheckBox
                android:id="@+id/yz_zx_zhu_she_cb4_3"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:button="@null"
                android:text="用法错误"
                android:gravity="center"
                android:textSize="20sp"
                android:layout_marginLeft="1dp"
                android:layout_marginRight="5dp"
                android:background="@drawable/yichang_bg_shape5"
                android:layout_height="match_parent"
                android:textColor="@color/abc_secondary_text_material_light" />
        </LinearLayout>
        <TextView
            android:layout_below="@+id/yz_zx_zhu_she_ll4"
            android:text="患者特殊状态"
            android:textSize="14sp"
            android:textColor="#99ffffff"
            android:gravity="center"
            android:layout_marginTop="5dp"
            android:layout_marginRight="5dp"
            android:layout_marginLeft="5dp"
            android:layout_width="match_parent"
            android:layout_height="20dp"/>
        <LinearLayout
            android:id="@+id/yz_zx_zhu_she_ll5"
            android:layout_below="@+id/yz_zx_zhu_she_ll4"
            android:orientation="horizontal"
            android:layout_marginTop="25dp"
            android:layout_width="match_parent"
            android:layout_height="45dp">
            <CheckBox
                android:id="@+id/yz_zx_zhu_she_cb5_1"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:button="@null"
                android:text="患者外出"
                android:gravity="center"
                android:textSize="20sp"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="1dp"
                android:background="@drawable/yichang_bg_shape1"
                android:layout_height="match_parent"
                android:textColor="@color/abc_secondary_text_material_light" />
            <CheckBox
                android:id="@+id/yz_zx_zhu_she_cb5_2"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:button="@null"
                android:text="患者拒绝"
                android:gravity="center"
                android:textSize="20sp"
                android:background="@drawable/yichang_bg_shape2"
                android:layout_height="match_parent"
                android:textColor="@color/abc_secondary_text_material_light" />
            <CheckBox
                android:id="@+id/yz_zx_zhu_she_cb5_3"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:button="@null"
                android:text="患者睡眠"
                android:gravity="center"
                android:textSize="20sp"
                android:layout_marginLeft="1dp"
                android:layout_marginRight="5dp"
                android:background="@drawable/yichang_bg_shape3"
                android:layout_height="match_parent"
                android:textColor="@color/abc_secondary_text_material_light" />
        </LinearLayout>
        <LinearLayout
            android:id="@+id/yz_zx_zhu_she_ll6"
            android:layout_below="@+id/yz_zx_zhu_she_ll5"
            android:orientation="horizontal"
            android:layout_marginTop="1dp"
            android:layout_width="match_parent"
            android:layout_height="45dp">
            <CheckBox
                android:id="@+id/yz_zx_zhu_she_cb6_1"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:button="@null"
                android:text="患者手术"
                android:gravity="center"
                android:textSize="20sp"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="1dp"
                android:background="@drawable/yichang_bg_shape4"
                android:layout_height="match_parent"
                android:textColor="@color/abc_secondary_text_material_light" />
            <CheckBox
                android:id="@+id/yz_zx_zhu_she_cb6_2"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:button="@null"
                android:text="病情改变"
                android:gravity="center"
                android:textSize="20sp"
                android:background="@drawable/yichang_bg_shape2"
                android:layout_height="match_parent"
                android:textColor="@color/abc_secondary_text_material_light" />
            <CheckBox
                android:id="@+id/yz_zx_zhu_she_cb6_3"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:button="@null"
                android:text="其他"
                android:gravity="center"
                android:textSize="20sp"
                android:layout_marginLeft="1dp"
                android:layout_marginRight="5dp"
                android:background="@drawable/yichang_bg_shape5"
                android:layout_height="match_parent"
                android:textColor="@color/abc_secondary_text_material_light" />
        </LinearLayout>
        <LinearLayout
            android:id="@+id/yz_zx_zhu_she_bt_ll"
            android:layout_width="match_parent"
            android:layout_alignParentBottom="true"
            android:orientation="horizontal"
            android:layout_height="45dp">

            <Button
                android:id="@+id/yz_zx_zhu_she_beizhu_bt"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:gravity="center"
                android:textColor="@color/abc_secondary_text_material_light"
                android:textSize="18sp"
                android:text="备注"
                android:background="@drawable/ti_zheng_duo_ren_bt_selater"
                android:layout_marginRight="1dp"
                android:layout_height="match_parent" />

            <Button
                android:id="@+id/yz_zx_zhu_she_zhixing_bt"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:gravity="center"
                android:background="@drawable/ti_zheng_duo_ren_bt_selater1"
                android:textColor="#fff"
                android:textSize="18sp"
                android:text="确认异常"
                android:layout_height="match_parent" />
        </LinearLayout>
    </RelativeLayout>
</LinearLayout>
